{
  "action": {
    "default_icon": "icons/icon.png",
    "default_popup": "index.html"
  },
  "author": "aljazmc",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{e1ea7933-f4a5-4d96-8838-90950e98d093}"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.humblebundle.com/*"],
      "js": ["content.js"]
    }
  ],
  "description": "A bulk downloader for humblebundle.com",
  "developer": {
    "name": "aljazmc",
    "url": "https://github.com/aljazmc"
  },
  "homepage_url": "https://github.com/aljazmc/humble-bundle-nice-downloader-firefox-extension",
  "host_permissions": ["*://*.humblebundle.com/*"],
  "icons": {
    "48": "icons/icon.svg",
    "72": "icons/icon.svg",
    "96": "icons/icon.svg",
    "128": "icons/icon.svg",
    "256": "icons/icon.svg"
  },
  "manifest_version": 3,
  "name": "Humble Bundle Nice Downloader",
  "permissions": [
    "activeTab",
    "downloads",
    "notifications",
    "storage"
  ],
  "short_name": "HBN Downloader",
  "version": "0.0.0"
}
